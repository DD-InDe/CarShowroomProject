<Page x:Class="CarShowroom.Pages.AdminMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:CarShowroom.Pages"
      mc:Ignorable="d"
      Title="Меню адмнистратора"
      d:DesignHeight="450"
      d:DesignWidth="800">

    <StackPanel Orientation="Vertical">
        <Button Style="{StaticResource Button-Style}"
                Content="Управление пользователями" />
        <Button Style="{StaticResource Button-Style}"
                Content="Управление автомобилями" />
        <Button Style="{StaticResource Button-Style}"
                Content="Управление заявками" />
        <Button Style="{StaticResource Button-Style}"
                Content="Отчет и статистика" />
    </StackPanel>
</Page>